mem_token=10G
for f in expt/horseRhoCrossVal/config*.m
do
	num=`basename $f .m | sed 's|config||'`
	name=hyper-search-$num
	log=qLogs/$name.log
	script=qScripts/$name.sh
	echo runHorseConfig.sh $num > $script
echo 	qsub -cwd  -j y  -o $log  -l mem_token=$mem_token  -N $name  -S /bin/sh $script
done

